<div class="campo-relatorio">
    <div class="header-relatorio">
        <h2>Relatorios</h2>
        <button type="button" onclick="print()" id="btnCadastrar">Imprimir</button>
    </div>

    <div class="tabela" id="tabelalista">
        <table>
            <thead>
            <th>Nome</th>
            <th>E-mail</th>
            </thead>
            <tbody>
                @foreach (var colaborador in Model)
                {
                    <tr>
                        <td>@colaborador.nomeColab</td>
                        <td>@colaborador.emailColab</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>